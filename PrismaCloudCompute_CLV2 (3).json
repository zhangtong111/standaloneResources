                      {
                        "name": "time",
                        "type": "datetime",
                        "description": "The time at which the data was generated"
                      },
                      {
                        "name": "fqdn",
                        "type": "string",
                        "description": "Fqdn."
                      },
                      {
                        "name": "containerName",
                        "type": "string",
