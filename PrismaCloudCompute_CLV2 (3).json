      "properties": {
        "description": "PrismaCloudCompute data connector with template",
        "displayName": "PrismaCloudCompute template"
      }
    },
    {
      "type": "Microsoft.Resources/templateSpecs/versions",
      "apiVersion": "2021-05-01",
